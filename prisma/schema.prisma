generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")  
}

// This model created the migration for the homepage carousel images

model HomepageCarouselImages {
  id          Int    @id @default(autoincrement())
  url         String @unique
  description String
  alt         String
}
